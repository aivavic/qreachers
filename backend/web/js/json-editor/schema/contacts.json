{
    "type": "object",
    "title": "Contacts",
    "id": "Contacts",
    "properties": {
        "tab_title": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "template": "{{title}}",
            "watch": {
                "title": "widget.___contacts___"
            }
        },
        "___contacts___": {            
            "type": "string",
            "options": {
                "hidden": true
            },            
            "default": "Contacts"
        },
        "contacts-preview": {
            "title": "Example:",
            "type": "string",
            "format": "hidden",
            "default": "/frontend/web/js/app/widgets/contacts/img/preview.jpg",
            "links": [
                {
                    "href": "{{self}}",
                    "mediaType": "image"
                }
            ]

        }
    }

}