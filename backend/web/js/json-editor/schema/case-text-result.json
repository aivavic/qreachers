{
    "type": "object",
    "title": "Case text result",
    "properties": {
        "tab_title": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "template": "{{title}}",
            "watch": {
                "title": "widget.___case-text-result___"
            }
        },
        "___case-text-result___": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "default": "Case text result"
        },
        "case-text-result-preview": {
            "title": "Example:",
            "type": "string",
            "format": "hidden",
            "default": "/frontend/web/js/app/widgets/case-text-result/img/preview.jpg",
            "links": [
                {
                    "href": "{{self}}",
                    "mediaType": "image"
                }
            ]

        },
        "title": {
            "type": "string",
            "title": "Title",
            "default": "Title ..."
        },
        "count": {
            "type": "string",
            "title": "Count",
            "default": "Count ..."
        },
        "title_count": {
            "type": "string",
            "title": "Title count",
            "default": "Title count ..."
        },
        "progress": {
            "type": "string",
            "title": "Progress",
            "default": "Progress ..."
        },
        "title_progress": {
            "type": "string",
            "title": "Title progress",
            "default": "Title progress ..."
        },
        "text": {
            "type": "string",
            "format": "html",
            "options": {
                "wysiwyg": true
            },
            "title": "Text.",
            "default": "Lorem ipsum dolor sit amet, consectetur adipiscing."
        }
    }
}